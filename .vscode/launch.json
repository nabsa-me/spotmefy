{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "TS Debugger",
        "type": "node",
        "request": "launch",
        "args": [
          "${relativeFile}"
        ],
        "runtimeArgs": [
          "-r",
          "ts-node/register"
        ],
        "cwd": "${workspaceRoot}",
        "internalConsoleOptions": "openOnSessionStart"
      }
    ]
  }